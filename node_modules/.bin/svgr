#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/amsterdam/amsterdam/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/amsterdam/amsterdam/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@svgr+cli@6.5.1/node_modules/@svgr/cli/bin/svgr" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@svgr+cli@6.5.1/node_modules/@svgr/cli/bin/svgr" "$@"
fi
